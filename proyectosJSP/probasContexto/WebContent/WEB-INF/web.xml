  </session-config>
  	<env-entry>
  		<env-entry-name>directorio</env-entry-name>
  		<env-entry-type>java.lang.String</env-entry-type>
  		<env-entry-value>C:\Cursos\ProgramacionJSP</env-entry-value>
  	</env-entry>
  	<env-entry>
  		<env-entry-name>username</env-entry-name>
  		<env-entry-type>java.lang.String</env-entry-type>
  		<env-entry-value>adminXXX</env-entry-value>
  	</env-entry>
</web-app>